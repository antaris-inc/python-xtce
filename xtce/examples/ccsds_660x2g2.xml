			</xtce:FloatParameterType>
			<xtce:FloatParameterType sizeInBits="64" name="PowmX0Bus_VoltageType">
				<xtce:UnitSet>
					<xtce:Unit power="1.0" factor="1"
description="ScaleUnits">V</xtce:Unit>
				</xtce:UnitSet>
				<xtce:IntegerDataEncoding encoding="twosComplement"
sizeInBits="16" bitOrder="mostSignificantBitFirst">
					<xtce:DefaultCalibrator>
