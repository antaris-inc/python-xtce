sizeInBits="16" bitOrder="mostSignificantBitFirst">
					<xtce:DefaultCalibrator>
						<xtce:PolynomialCalibrator>
							<xtce:Term coefficient="0.0"
exponent="0"/>
							<xtce:Term coefficient="0.1"
exponent="1"/>
