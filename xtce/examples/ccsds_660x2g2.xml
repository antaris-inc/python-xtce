name="PowmX0EP_PowerStateType">
				<xtce:IntegerDataEncoding encoding="unsigned" sizeInBits="8"
bitOrder="mostSignificantBitFirst"/>
				<xtce:EnumerationList>
					<xtce:Enumeration value="1" label="Off"/>
					<xtce:Enumeration value="2" label="On"/>
					<xtce:Enumeration value="3" label="Tripped"/>
					<xtce:Enumeration value="4" label="NotWorking"/>
				</xtce:EnumerationList>
			</xtce:EnumeratedParameterType>
			<xtce:FloatParameterType sizeInBits="64" name="PowmX0EP_SetPointType">
				<xtce:UnitSet>
